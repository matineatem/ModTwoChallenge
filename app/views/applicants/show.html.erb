<h1>applicant#show</h1>

<h2>Name: <%=@applicant.display_name%> </h2>
<h2>Email: <%=@applicant.email%> </h2>
<h2>Empolyers:<h2>
<ol>
    <% if @applicant.employers.length != 0 %>
        <% @applicant.employers.each do |employer| %>
            <li><%= employer.display_name %></li>
        <% end %>
    <% else %>
        <h3>This Applicant has no employers.<h3>
    <% end %>
</ol>